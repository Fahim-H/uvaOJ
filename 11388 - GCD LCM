#include <iostream>
#define FastAsRocket ios::sync_with_stdio(false);cin.tie(0);cout.tie(0)
#define ll long long int
using namespace std;

int main(){
    FastAsRocket;
    ll t; cin>>t;
    while(t--){
        ll G, L;
        cin>>G>>L;
        if(L%G == 0)cout<<G<<" "<<L<<endl;
        else cout<<-1<<endl;
    }
    return 0;
}
